# Audio
persist.vendor.audio.fluence.voicecomm=true
persist.vendor.audio.voicecall.speaker.stereo=false

# Camera
persist.vendor.camera.physical.num=3

# Display
debug.sf.frame_rate_multiple_threshold=120
debug.sf.high_speed_scroll_factor=0
ro.surface_flinger.has_wide_color_display=false
ro.surface_flinger.set_idle_timer_ms=500
ro.surface_flinger.uclamp.min=512
ro.vendor.display.sensortype=2
ro.vendor.display.svi=1
vendor.display.svi.config=1
vendor.display.svi.config_path=/vendor/etc/SVIConfig.xml

# Fingerprint
vendor.false_touch_quality=38

# NFC
persist.vendor.nfc.config_file_name=libnfc-SN220-UICC.conf

# Radio
ro.vendor.radio.imei.sv=1
