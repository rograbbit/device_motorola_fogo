        <item>130</item>
        <item>150</item>
        <item>180</item>
        <item>180</item>
        <item>180</item>
        <item>180</item>
        <item>210</item>
        <item>210</item>
        <item>210</item>
        <item>220</item>
        <item>225</item>
        <item>225</item>
        <item>350</item>
        <item>400</item>
        <item>400</item>
        <item>400</item>
        <item>400</item>
    </integer-array>

